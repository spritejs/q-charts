{
  "name": "@qcharts/core",
  "version": "0.1.39",
  "description": "A visulization library based on spritejs and sprite-extend-shapes",
  "main": "lib/index.js",
  "miniprogram": "miniprogram",
  "scripts": {
    "start": "npm run dev:doc",
    "dev:doc": "cross-env DEV_MODE=doc webpack-dev-server --config ./build/webpack.conf.dev.js",
    "dev:demo": "webpack-dev-server --config ./build/webpack.conf.dev.js",
    "build": "npm run build:docs && npm run build:chart && echo 'Build successfully!'",
    "build:chart": "rimraf lib && cross-env NODE_ENV=production webpack --config ./build/webpack.conf.prod.chart.js",
    "build:wxapp": "cross-env NODE_ENV=production webpack --config ./build/webpack.conf.prod.chart.wxapp.js",
    "build:chart:rollup": "rollup -c rollup.config.js",
    "build:docs": "rimraf docs && cross-env NODE_ENV=production webpack --config ./build/webpack.conf.prod.docs.js",
    "prepublishOnly": "npm run build:chart && npm run build:wxapp",
    "test": "nyc ava -v --serial && npm run testReport",
    "testReport": "rm -rf ./coverage && mkdirp ./coverage && nyc report --reporter=html > ./coverage/lcov.info",
    "testOnly": "cross-env NODE_ENV=test ava -v",
    "lint": "eslint src --fix"
  },
  "author": "qwt",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.4.4",
    "@babel/runtime": "^7.2.0",
    "ava": "^1.4.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-inline-package-json": "^2.0.0",
    "babel-plugin-webpack-alias-7": "^0.1.1",
    "bootstrap": "^4.3.1",
    "canvas": "^2.4.1",
    "canvas-5-polyfill": "^0.1.5",
    "codemirror": "^5.42.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.1",
    "eslint-config-ideal": "^1.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^3.0.1",
    "happypack": "^5.0.1",
    "highlight.js": "^9.13.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.0",
    "markdown-it": "^8.4.2",
    "markdown-it-container": "^2.0.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.11.0",
    "nyc": "^14.1.0",
    "progress-bar-webpack-plugin": "^1.12.1",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.6.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-uglify": "^6.0.2",
    "sass-loader": "^7.1.0",
    "scapegoat": "^1.0.0",
    "split.js": "^1.5.10",
    "sprite-core": "^2.31.3",
    "spritejs": "^2.27.13",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.5.1",
    "vue-md-loader": "^1.1.0",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.28.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.1.5"
  },
  "dependencies": {
    "@spritejs/shapes": "^1.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
